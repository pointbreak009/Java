public class GreatestNumber
{
   public static void main(String[] args)
   {
       int a=24;
       int b=71;
       int c=20;
       
       if(a>=b&&a>=c)
   {
       System.out.print(a+ " is greatest number");
   }
   else if(b>=a&&b>=c)
   {
       System.out.print(b+ " is greatest number");
   }
   else
   {
       System.out.print(c+ " is greatest number");
   }
   }
   }
